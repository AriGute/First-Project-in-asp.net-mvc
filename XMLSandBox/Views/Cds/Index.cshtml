@model XMLSandBox.Models.CdList

<h1>Cds</h1>
<div>
    <table>
        <tr >
            <th>
                <label >Title</label>
            </th>
            <th>
                <label >Artist</label>
            </th>
            <th>
                <label >Country</label>
            </th>
            <th>
                <label >Price</label>
            </th>
            <th>
                <label >Year</label>
            </th>
        </tr>

        @foreach (var item in Model.cdList)
        {
            <tr>
                <td>@item.TITLE</td>
                <td>@item.ARTIST</td>
                <td>@item.COUNTRY</td>
                <td>@item.PRICE</td>
                <td>@item.YEAR</td>
            </tr>
        }
    </table>
</div>